#include <iostream>

using namespace std;

int main()
{
    int x;
    cin>>x;
    if (x<0 && x%2==0)
    cout<<"Отрицательное четное число"; 
    if (x<0 && x%2!=0)
    cout<<"Отрицательное нечетное число"; 
    if (x>0 && x%2==0)
    cout<<"Положительное четное число"; 
    if (x>0 && x%2!=0)
    cout<<"Положительное нечетное число";
    if (x==0)
    cout<<"Нулевое число"; 
    return 0;
}
